{
  "formId": {
	"brokerId": "general",
	"policyCategory": "non-sport"
  },
  "schema": {
	"type": "object",
	"title": "",
	"properties": {
	  "number-of-participants": {
		"title": "What is the maximum number of participants attending the event?",
		"type": "integer",
		"minimum": 1,
		"default": 0
	  },
	  "number-of-vehicles": {
		"title": "How many rental short term rental vehicles require physical damage coverage?",
		"type": "integer",
		"minimum": 1,
		"default": 0
	  },
	  "activities-per-week": {
		"title": "How many events take place weekly?",
		"type": "integer",
		"minimum": 1,
		"maximum": 2,
		"default": 0
	  },
	  "hours-per-activity": {
		"title": "How many hours is each event that takes place weekly?",
		"type": "integer",
		"minimum": 1,
		"maximum": 10,
		"default": 0
	  },
	  "social-alcohol": {
		"title": "Will the event have alcohol being served?",
		"type": "radiobuttons",
		"titleMap": [
		  {
			"value": "alcohol-caterer",
			"name": "Yes - With Licenced Caterer"
		  },
		  {
			"value": "alcohol-no-caterer",
			"name": "Yes - Without Licenced Caterer"
		  },
		  {
			"value": "no-alcohol",
			"name": "No - No Alcohol"
		  }
		],
		"default": null
	  }
	}
  },
  "form": [
	{
	  "key": "yearly-coverage",
	  "title": "Please select the type of coverage you wish to purchase:",
	  "type": "select",
	  "titleMap": [
		{
		  "value": false,
		  "name": "Single Event"
		},
		{
		  "value": true,
		  "name": "Weekly / Full Year"
		}
	  ],
	  "default": false,
	  "required": true,
	  "condition": "model.VALID_ACTIVITIES.non_sports.allow_weekly == true && model.selectedActivityType == 'class-1'"
	},
	{
	  "key": "activities-per-week",
	  "fieldHtmlClass": "signup-button",
	  "required": true,
	  "condition": "model.selectedActivityType == 'class-1' && model['yearly-coverage']"
	},
	{
	  "key": "hours-per-activity",
	  "fieldHtmlClass": "signup-button",
	  "required": true,
	  "condition": "model.selectedActivityType == 'class-1' && model['yearly-coverage']"
	},
	{
	  "key": "social-alcohol",
	  "title": "Will the event have alcohol or cannabis being served?",
	  "type": "select",
	  "fieldHtmlClass": "input-border",
	  "titleMap": [
		{
		  "value": "alcohol-caterer",
		  "name": "Yes - Alcohol Served by Venue or Licenced Caterer"
		},
		{
		  "value": "alcohol-no-caterer",
		  "name": "Yes - Self Served Alcohol"
		},
		{
		  "value": "cannabis-caterer",
		  "name": "Yes - Cannabis Served by Licensed Caterer"
		},
		{
		  "value": "no-alcohol",
		  "name": "No - No Alcohol or Cannabis"
		}
	  ],
	  "condition": "model.VALID_ACTIVITIES.alcohol.enabled == true && (model.selectedActivityType == 'class-1' || model.selectedActivityType == 'class-2')",
	  "required": true
	},
	{
	  "key": "social-alcohol",
	  "title": "Will the event have alcohol or cannabis being served?",
	  "type": "select",
	  "fieldHtmlClass": "input-border",
	  "titleMap": [
		{
		  "value": "no-alcohol",
		  "name": "No - No Alcohol or Cannabis"
		}
	  ],
	  "condition": "model.VALID_ACTIVITIES.alcohol.enabled == false && (model.selectedActivityType == 'class-1' || model.selectedActivityType == 'class-2')",
	  "required": true
	},
	{
	  "key": "requires-5-mil",
	  "title": "Please select the amount of insurance coverage required:",
	  "type": "select",
	  "titleMap": [
		{
		  "value": false,
		  "name": "$2,000,000"
		},
		{
		  "value": true,
		  "name": "$5,000,000"
		}
	  ],
	  "default": false,
	  "required": true,
	  "condition": "model['social-alcohol'] === 'no-alcohol' && model.VALID_ACTIVITIES.non_sports.limits['2'] == true && model.VALID_ACTIVITIES.non_sports.limits['5'] == true"
	},
	{
	  "key": "requires-5-mil",
	  "title": "Please select the amount of insurance coverage required:",
	  "type": "select",
	  "titleMap": [
		{
		  "value": true,
		  "name": "$5,000,000"
		}
	  ],
	  "default": false,
	  "required": true,
	  "condition": "model['social-alcohol'] === 'no-alcohol' && model.VALID_ACTIVITIES.non_sports.limits['2'] == false && model.VALID_ACTIVITIES.non_sports.limits['5'] == true"
	},
	{
	  "key": "requires-5-mil",
	  "title": "Please select the amount of insurance coverage required:",
	  "type": "select",
	  "titleMap": [
		{
		  "value": false,
		  "name": "$2,000,000"
		}
	  ],
	  "default": false,
	  "required": true,
	  "condition": "model['social-alcohol'] === 'no-alcohol' && model.VALID_ACTIVITIES.non_sports.limits['2'] == true && model.VALID_ACTIVITIES.non_sports.limits['5'] == false"
	},
	{
	  "key": "requires-5-mil",
	  "title": "Please select the amount of insurance coverage required:",
	  "type": "select",
	  "titleMap": [
		{
		  "value": false,
		  "name": "$2,000,000"
		},
		{
		  "value": true,
		  "name": "$5,000,000"
		}
	  ],
	  "default": false,
	  "required": true,
	  "condition": "model['social-alcohol'] != 'no-alcohol' && model.VALID_ACTIVITIES.alcohol.limits['2'] == true && model.VALID_ACTIVITIES.alcohol.limits['5'] == true"
	},
	{
	  "key": "requires-5-mil",
	  "title": "Please select the amount of insurance coverage required:",
	  "type": "select",
	  "titleMap": [
		{
		  "value": true,
		  "name": "$5,000,000"
		}
	  ],
	  "default": false,
	  "required": true,
	  "condition": "model['social-alcohol'] != 'no-alcohol' && model.VALID_ACTIVITIES.alcohol.limits['2'] == false && model.VALID_ACTIVITIES.alcohol.limits['5'] == true"
	},
	{
	  "key": "requires-5-mil",
	  "title": "Please select the amount of insurance coverage required:",
	  "type": "select",
	  "titleMap": [
		{
		  "value": false,
		  "name": "$2,000,000"
		}
	  ],
	  "default": false,
	  "required": true,
	  "condition": "model['social-alcohol'] != 'no-alcohol' && model.VALID_ACTIVITIES.alcohol.limits['2'] == true && model.VALID_ACTIVITIES.alcohol.limits['5'] == false"
	},
	{
	  "key": "birthday-info",
	  "type": "help",
	  "helpvalue": "<b><a href='https://support.instantriskcoverage.com/en/knowledge-base/what-should-i-select-if-i-have-both-a-gym-and-room-rental' target='_blank'><u>If you are having a birthday party with a gym/pool or other space rental as well</u></a>, please ensure to add one-time sporting coverage for the time you will be using the gym</b>",
	  "condition": "model.selectedActivityId == 'birthday-party'"
	},
	{
	  "key": "block-party-info",
	  "type": "help",
	  "helpvalue": "<b>If you are having extra amenities at your block party such as petting zoos or inflatables, you must add multiple vendor coverage as well after you complete this application</b>",
	  "condition": "model.selectedActivityId == 'block-party'"
	},
	{
	  "key": "number-of-participants",
	  "fieldHtmlClass": "signup-button",
	  "required": true
	},
	{
	  "key": "sop",
	  "title": "I acknowledge that I must submit my Liquor License to facility staff. I also acknowledge that if the Liquor License is not submitted to facility staff before the booking insurance will not be in effect and the booking may be cancelled.",
	  "type": "radiobuttons",
	  "titleMap": [
		{
		  "value": true,
		  "name": "Yes"
		},
		{
		  "value": false,
		  "name": "No"
		}
	  ],
	  "required": true,
	  "description": "<a href='https://support.instantriskcoverage.com/en/knowledge-base/what-is-a-liquor-licence-in-my-province' target='_blank' class='mt-3'>What is a liquor license in my province <i class='fa-solid fa-circle-question'></i></a>",
	  "condition": "model['social-alcohol'] != 'no-alcohol' && model['social-alcohol'] != 'cannabis-caterer' && model['social-alcohol'] != null"
	},
	{
	  "key": "smart-serve",
	  "title": "I acknowledge those serving alcohol to others have their bartending certification and have provided you with a copy.",
	  "type": "radiobuttons",
	  "titleMap": [
		{
		  "value": true,
		  "name": "Yes"
		},
		{
		  "value": false,
		  "name": "No"
		},
		{
		  "value": "na",
		  "name": "N/A"
		}
	  ],
	  "required": true,
	  "description": "If participants are self serving alcohol then bartending certification is not required. If this is the case select N/A. <a href='https://support.instantriskcoverage.com/en/knowledge-base/what-is-a-bartending-certification-in-my-province' target='_blank' class='mt-3'>Click here to find out what the bartending certification in my province is<i class='ml-1 fa-solid fa-circle-question'></i></a>",
	  "condition": "model['social-alcohol'] != 'no-alcohol' && model['social-alcohol'] != 'cannabis-caterer' && model['social-alcohol'] != null"
	},
	{
	  "key": "cannabis-certification",
	  "title": "I acknowledge those serving cannabis to others have proper certification and have provided you with a copy.",
	  "type": "radiobuttons",
	  "titleMap": [
		{
		  "value": true,
		  "name": "Yes"
		},
		{
		  "value": false,
		  "name": "No"
		}
	  ],
	  "required": true,
	  "condition": "model['social-alcohol'] == 'cannabis-caterer' && model['social-alcohol'] != null"
	},
	{
	  "key": "alcohol-receipts",
	  "title": "I acknowledge that alcohol receipts will not exceed $10,000 per day.",
	  "type": "radiobuttons",
	  "default": false,
	  "titleMap": [
		{
		  "value": true,
		  "name": "Yes"
		},
		{
		  "value": false,
		  "name": "No"
		}
	  ],
	  "required": true,
	  "condition": "model['social-alcohol'] != 'no-alcohol' && model['social-alcohol'] != 'cannabis-caterer' && model['social-alcohol'] != null"
	},
	{
	  "key": "non-owned-liability",
	  "title": "Please select the amount of non-owned auto insurance coverage you wish to add:",
	  "type": "select",
	  "titleMap": [
		{
		  "value": 1,
		  "name": "$1,000,000"
		},
		{
		  "value": 2,
		  "name": "$2,000,000"
		},
		{
		  "value": 5,
		  "name": "$5,000,000"
		}
	  ],
	  "default": 1,
	  "required": true,
	  "condition": "model['non-owned-auto'] == true"
	},
	{
	  "key": "non-owned-valid-license",
	  "title": "Does every driver of the vehicles have a valid drivers' license?",
	  "type": "radiobuttons",
	  "default": false,
	  "titleMap": [
		{
		  "value": true,
		  "name": "Yes"
		},
		{
		  "value": false,
		  "name": "No"
		}
	  ],
	  "required": true,
	  "condition": "model['non-owned-auto'] == true"
	},
	{
	  "key": "non-owned-transport",
	  "title": "Are you responsible for providing transportation and/or chauffeuring services, or transporting attendees?",
	  "type": "radiobuttons",
	  "default": false,
	  "titleMap": [
		{
		  "value": true,
		  "name": "Yes"
		},
		{
		  "value": false,
		  "name": "No"
		}
	  ],
	  "required": true,
	  "condition": "model['non-owned-auto'] == true"
	},
	{
	  "key": "non-owned-renting",
	  "title": "Do you have any short term rental vehicles (less than 30 days)?",
	  "type": "radiobuttons",
	  "default": false,
	  "titleMap": [
		{
		  "value": true,
		  "name": "Yes"
		},
		{
		  "value": false,
		  "name": "No"
		}
	  ],
	  "required": true,
	  "condition": "model['non-owned-auto'] == true"
	},
	{
	  "key": "non-owned-physical-damage",
	  "title": "Do you require physical damage coverage for the short term rental vehicles?",
	  "type": "radiobuttons",
	  "default": false,
	  "titleMap": [
		{
		  "value": true,
		  "name": "Yes"
		},
		{
		  "value": false,
		  "name": "No"
		}
	  ],
	  "required": true,
	  "condition": "model['non-owned-auto'] == true && model['non-owned-renting'] == true"
	},
	{
	  "key": "number-of-vehicles",
	  "fieldHtmlClass": "signup-button",
	  "required": true,
	  "condition": "model['non-owned-auto'] == true && model['non-owned-physical-damage'] == true"
	},
	{
	  "key": "non-owned-physical-damage-amount",
	  "title": "What limit of physical damage coverage is required per vehicle?",
	  "type": "select",
	  "titleMap": [
		{
		  "value": 25,
		  "name": "$25,000"
		},
		{
		  "value": 50,
		  "name": "$50,000"
		},
		{
		  "value": 100,
		  "name": "$100,000"
		}
	  ],
	  "default": 25,
	  "required": true,
	  "condition": "model['non-owned-auto'] == true && model['non-owned-physical-damage'] == true"
	},
	{
	  "type": "submit",
	  "style": "hidden FormSubmitButton",
	  "title": "OK"
	}
  ]
}
